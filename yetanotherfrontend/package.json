{
  "name": "yetanotherfrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "npm install && concurrently \"cd browser && npm install\"",
    "dev": "concurrently \"npm run next\" \"npm run browserdev\"",
    "next": "next dev",
    "browserdev": "cd browser && neu run",
    "all": "npm run setup && npm run dev",
    "test:start": "concurrently --kill-others --success first \"npm install && npm run next\" \"wait-on http://localhost:3000 && npm run test:response\"",
    "test:response": "npx mocha __test__/*.js"
  },
  "main": "main.js",
  "dependencies": {
    "@hookform/resolvers": "^4.0.0",
    "@neutralinojs/neu": "^11.3.0",
    "@playwright/test": "^1.47.2",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "file-system": "^2.2.2",
    "fs": "^0.0.1-security",
    "lucide-react": "^0.468.0",
    "next": "14.2.21",
    "next-themes": "^0.4.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.3.0",
    "react-toastify": "^10.0.5",
    "sonner": "^2.0.0",
    "tailwind-merge": "^3.0.0",
    "tailwindcss-animate": "^1.0.7",
    "yetanotherfrontend": "file:",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "chai": "^4.3.7",
    "concurrently": "^6.5.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.20",
